<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>Controlador_F04</servlet-name>
        <servlet-class>controlador.Controlador_F04</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Controlador_F16</servlet-name>
        <servlet-class>controlador.Controlador_F16</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sesion</servlet-name>
        <servlet-class>controlador.Sesion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Controlador_F04</servlet-name>
        <url-pattern>/Grupos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Controlador_F16</servlet-name>
        <url-pattern>/Proyectos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sesion</servlet-name>
        <url-pattern>/sesion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
